func get_word(type, index = randi_range(1,50)):
	if index < 1 or index > 50:
		index = abs(index) % 50
		
	if type == 'adjective':
		return adjectives[index]
	elif type == 'noun':
		return nouns[index]
	elif type == 'verb':
		return verbs[index]
	elif type == 'exclamation':
		return exclamations[index]
	elif type == 'plural_noun':
		return plural_nouns[index]
	elif type == 'past_verb':
		return past_verbs[index]
	elif type == 'verb_ing':
		return verb_ing[index]
	else:
		return 'fail'



